{
  "name": "AutoI18nForNextJs",
  "description": "AutoI18n For NextJs. Desktop version",
  "version": "1.2.4",
  "private": true,
  "type": "module",
  "author": {
    "email": "support@autoi18n.dev",
    "name": "Kamjin",
    "url": "https://autoi18n.dev"
  },
  "main": "packages/main/dist/index.js",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config electron-builder.yml --dir --config.asar=false",
    "dist": "cross-env MODE=production npm run build && electron-builder build --config electron-builder.yml --config.asar=true",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "patch-package && node scripts/watch.js",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js && patch-package",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\"",
    "start": "npm run build && patch-package && electron ."
  },
  "devDependencies": {
    "@types/node": "20.14.2",
    "@types/prismjs": "^1.26.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "^10.4.19",
    "cross-env": "7.0.3",
    "electron": "^31.0.0",
    "electron-builder": "24.13.3",
    "electron-icon-builder": "^2.0.1",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.26.0",
    "happy-dom": "14.12.0",
    "nano-staged": "0.8.0",
    "patch-package": "^8.0.0",
    "playwright": "1.44.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5",
    "unplugin-auto-expose": "0.3.0",
    "vite": "^5.2.13",
    "vitest": "1.6.0",
    "vue": "3.4.27",
    "vue-tsc": "2.0.19"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/generator": "^7.23.6",
    "@babel/helper-environment-visitor": "^7.24.7",
    "@babel/helper-plugin-utils": "^7.24.0",
    "@babel/parser": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.24.1",
    "@babel/plugin-syntax-typescript": "^7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/template": "^7.24.7",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@heroicons/vue": "^2.1.4",
    "@tailwindcss/forms": "^0.5.7",
    "@typescript-eslint/parser": "^7.12.0",
    "async": "^3.2.5",
    "axios": "^1.6.8",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.1.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "6.2.1",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.9.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "marked": "^12.0.2",
    "npm": "^10.5.1",
    "open": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss-filters": "^3.0.0",
    "trucker": "^1.1.2",
    "uuid": "^9.0.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "yaml": "^2.4.1",
    "yargs": "^17.7.2"
  }
}
